# Backend API Configuration
EXPO_PUBLIC_API_URL=http://localhost:3000/api
EXPO_PUBLIC_WS_URL=ws://localhost:3000

# Replace with your actual backend URL for production
# EXPO_PUBLIC_API_URL=https://your-production-backend.com/api
# EXPO_PUBLIC_WS_URL=wss://your-production-backend.com

# Stripe (if using Stripe on frontend)
EXPO_PUBLIC_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key_here

# Feature Flags
EXPO_PUBLIC_ENABLE_BACKEND=true
EXPO_PUBLIC_ENABLE_MOCK_DATA=false
EXPO_PUBLIC_ENABLE_AI_FEATURES=true
